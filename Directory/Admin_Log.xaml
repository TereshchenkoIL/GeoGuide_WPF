<Window x:Class="Directory.Admin_Log"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Directory"
        mc:Ignorable="d"
        Title="Admin_Log" Height="300" Width="300">
    <Grid>
        <Label Content="Вхід для адміна" HorizontalAlignment="Left" Height="35" Margin="80,10,0,0" VerticalAlignment="Top" Width="170" FontSize="20"/>
        <Label Content="name" HorizontalAlignment="Left" Height="25" Margin="40,80,0,0" VerticalAlignment="Top" Width="45"/>
        <Label Content="password" HorizontalAlignment="Left" Height="25" Margin="35,135,0,0" VerticalAlignment="Top" Width="60"/>
        <TextBox x:Name="Admin_name" HorizontalAlignment="Left" Height="25" Margin="105,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="95"/>
        <PasswordBox x:Name="Admin_pas" HorizontalAlignment="Left" Height="25" Margin="105,135,0,0" VerticalAlignment="Top" Width="95"/>
        <Button Content="Війти" HorizontalAlignment="Left" Height="35" Margin="80,205,0,0" VerticalAlignment="Top" Width="140" Click="Button_Click"/>

    </Grid>
</Window>
